<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="examBox" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Pane style="-fx-background-color: #4e5c7e; -fx-width: 10000;">
         <children>
            <ImageView fitHeight="34.0" fitWidth="34.0" layoutX="14.0" layoutY="13.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../Images/home.png" />
               </image>
            </ImageView>
            <Label layoutX="48.0" layoutY="18.0" prefHeight="34.0" prefWidth="53.0" text="主界面" textFill="WHITE">
               <font>
                  <Font name="Heiti SC Medium" size="16.0" />
               </font>
            </Label>
            <ImageView fitHeight="24.0" fitWidth="17.0" layoutX="101.0" layoutY="27.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../Images/rightArrow.png" />
               </image>
            </ImageView>
            <Label layoutX="125.0" layoutY="18.0" prefHeight="34.0" prefWidth="70.0" text="考核管理" textFill="WHITE">
               <font>
                  <Font name="Heiti SC Medium" size="16.0" />
               </font>
            </Label>
            <ImageView fitHeight="17.0" fitWidth="17.0" layoutX="195.0" layoutY="27.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../Images/rightArrow.png" />
               </image>
            </ImageView>
            <Label layoutX="219.0" layoutY="18.0" prefHeight="34.0" prefWidth="70.0" text="考核统计" textFill="WHITE">
               <font>
                  <Font name="Heiti SC Medium" size="16.0" />
               </font>
            </Label>
         </children>
      </Pane>
      <HBox maxHeight="50.0" prefHeight="50.0" prefWidth="200.0">
         <children>
            <TextField fx:id="searchField" prefWidth="300.0">
               <HBox.margin>
                  <Insets top="10.0" />
               </HBox.margin>
            </TextField>
            <Button mnemonicParsing="false" text="搜索">
               <HBox.margin>
                  <Insets left="40.0" top="10.0" />
               </HBox.margin>
            </Button>
            <Button text="重置">
               <HBox.margin>
                  <Insets left="10.0" top="10.0" />
               </HBox.margin></Button>
               <Pane HBox.hgrow="ALWAYS" />
            <Button mnemonicParsing="false" text="新建试卷">
               <HBox.margin>
                  <Insets left="10.0" top="10.0" />
               </HBox.margin>
            </Button>
            <Button mnemonicParsing="false" text="发布试卷">
               <HBox.margin>
                  <Insets left="10.0" top="10.0" />
               </HBox.margin>
            </Button>
         </children>
         <VBox.margin>
            <Insets left="20.0" right="20.0" />
         </VBox.margin></HBox>
      <TableView fx:id="tableView" prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
         <VBox.margin>
            <Insets left="20.0" right="20.0" />
         </VBox.margin>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <HBox maxHeight="50.0" prefHeight="50.0" prefWidth="200.0" VBox.vgrow="NEVER">
         <children>
            <Label fx:id="countLabel" text="条目数量">
               <HBox.margin>
                  <Insets left="20.0" />
               </HBox.margin>
            </Label>
            <Pane HBox.hgrow="ALWAYS" />
            <Button mnemonicParsing="false" text="选择用户">
               <HBox.margin>
                  <Insets right="10.0" />
               </HBox.margin></Button>
            <Button layoutX="1232.0" layoutY="10.0" mnemonicParsing="false" text="编辑" />
            <Button mnemonicParsing="false" text="删除">
               <HBox.margin>
                  <Insets left="10.0" />
               </HBox.margin>
            </Button>
         </children>
         <VBox.margin>
            <Insets bottom="10.0" right="20.0" top="10.0" />
         </VBox.margin></HBox>
   </children>
</VBox>
